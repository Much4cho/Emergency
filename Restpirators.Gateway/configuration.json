{
  "Routes": [
    {
      "DownstreamPathTemplate": "/statistics",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "analyzer",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/statistics",
      "UpstreamHttpMethod": [ "GET" ],
      "SwaggerKey": "statistics"
    },
    {
      "DownstreamPathTemplate": "/statistics/{year}/{month}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "analyzer",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/statistics/{year}/{month}",
      "UpstreamHttpMethod": [ "GET" ],
      "SwaggerKey": "statistics"
    },
    {
      "DownstreamPathTemplate": "/statistics/getTimeStatistics",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "analyzer",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/statistics/getTimeStatistics",
      "UpstreamHttpMethod": [ "GET" ],
      "SwaggerKey": "statistics"
    },
    {
      "DownstreamPathTemplate": "/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "client",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/client/{everything}",
      "UpstreamHttpMethod": [ "POST" ],
      "SwaggerKey": "client"
    },
    {
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "dispatcher",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/dispatcher/{everything}",
      "UpstreamHttpMethod": [ "GET", "PUT", "POST" ],
      "SwaggerKey": "dispatcher"
    },
    {
      "DownstreamPathTemplate": "/users/authenticate",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "auth",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/authenticate",
      "UpstreamHttpMethod": [ "POST" ],
      "SwaggerKey": "auth"
    },
    {
      "DownstreamPathTemplate": "/users",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "auth",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/users",
      "UpstreamHttpMethod": [ "GET" ],
      "SwaggerKey": "auth"
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "statistics",
      "Config": [
        {
          "Name": "Statistics API",
          "Version": "v1",
          "Url": "http://analyzer/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "client",
      "Config": [
        {
          "Name": "Client API",
          "Version": "v1",
          "Url": "http://client/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "dispatcher",
      "Config": [
        {
          "Name": "Dispatcher API",
          "Version": "v1",
          "Url": "http://dispatcher/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "auth",
      "Config": [
        {
          "Name": "Auth API",
          "Version": "v1",
          "Url": "http://auth/swagger/v1/swagger.json"
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://gateway"
